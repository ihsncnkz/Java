import java.util.Scanner;

public class parola {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String parola = "";
		
		do {
			System.out.print("Lütfen bir parolanızı giriniz: ");
			parola = scanner.nextLine();
			
		}while (parola != "1234");
		System.out.println("Tebrikler!");
		
	}
}
