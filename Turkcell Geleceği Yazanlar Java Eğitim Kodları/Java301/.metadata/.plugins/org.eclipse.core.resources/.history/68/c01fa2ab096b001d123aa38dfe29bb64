import java.util.Vector;

public class Driver {

	public static void main(String[] args) {
		Vector<String> vector = new Vector<>();
		vector.add("Kedi");
		vector.add("Kopek");
		vector.add("Kaplan");
		
		for (int i = 0; i < vector.size(); i++) {
			String string = vector.get(i);
			System.out.println((i) + " " + string);
		}
	}
}
