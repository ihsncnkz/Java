import java.util.Scanner;

public class Driver {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int starCount = scanner.nextInt();
		int i = 1;
		while(i <= starCount) {
			int j = 0;
			while(j<i) {
				System.out.print("*");
				j++;
			}
			System.out.println();
			i++;
		}
	}
}
