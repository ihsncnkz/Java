
public class Araba {

	private String marka;
	private String model;
	private int yil;
	
	public String getMarka() {
		return marka;
	}
	public void setMarka(String marka) {
		this.marka = marka;
	}
	
	public String getModel() {
		return model;
	}
	public void setModel(String model) {
		this.model = model;
	}
	
	public int getYil() {
		return yil;
	}
	public void seYil(String yil) {
		this.yil = yil;
	}
}
